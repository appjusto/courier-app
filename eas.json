{
  "cli": {
    "version": ">= 3.15.1"
  },
  "build": {
    "base": {
      "channel": "v13"
    },
    "devclient": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "internal": {
      "extends": "base",
      "distribution": "internal"
    },
    "debug": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "store": {
      "extends": "base",
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
